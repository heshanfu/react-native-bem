{
  "name": "react-native-bem",
  "version": "0.2.0",
  "description": "A BEM-inspired method to styling React Native components",
  "main": "lib/index.js",
  "watch": {
    "build": "src/**/*.*"
  },
  "scripts": {
    "build": "babel src -d lib",
    "watch": "npm-watch",
    "lint": "eslint -c .eslintrc \"src/**/*.js\"; exit 0",
    "lint:flow": "npm-run-all lint flow",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "author": "Matt Stow <matt@mattstow.com>",
  "homepage": "http://mattstow.com",
  "license": "MIT",
  "repository": "https://github.com/stowball/react-native-bem.git",
  "keywords": [
    "react",
    "react-native",
    "bem",
    "style",
    "stylesheet"
  ],
  "dependencies": {
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "eslint": "^3.15.0",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-react": "^6.9.0",
    "flow-bin": "^0.39.0",
    "npm-run-all": "^4.0.1",
    "npm-watch": "^0.1.8",
    "react": "^15.4.2"
  },
  "peerDependencies": {
    "react": ">=15.3.0"
  }
}
